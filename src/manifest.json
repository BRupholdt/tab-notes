{
  "name": "Tab Notes",
  "description": "Write a new note in every new tab. Capture your thoughts faster.",
  "version": "0.4.1",
  "offline_enabled": true,
  "manifest_version": 2,
  "icons": {
    "16": "images/notes_16.png",
    "128": "images/icon_128.png"
  },
  "chrome_url_overrides" : {
    "newtab": "newtab/newtab.html"
  },
  "browser_action": {
    "default_popup": "browser_action/popup.html#default",
    "default_icon": {
      "16": "images/notes_48.png"
    }
  }
}

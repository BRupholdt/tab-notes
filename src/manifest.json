{
  "name": "Tab Notes",
  "description": "Notes in place of every new tab",
  "version": "0.2",
  "manifest_version": 2,
  "icons": {
    "16": "images/notes-16.png",
    "128": "images/icon_128.png"
  },
  "chrome_url_overrides" : {
    "newtab": "newtab/newtab.html"
  },
  "browser_action": {
    "default_popup": "browser_action/popup.html",
    "default_icon": {
      "16": "images/notes-48.png"
    }
  }
}
